@import "colores"
header
	background azulPrimario
	color blanco
	position relative
	img
		width 15%
		display inline-block
	div.titulos
		text-align center
		width 82%
		display inline-block
		vertical-align top
		
	nav
		display none
		li
			list-style none
			padding 1%
	div.clickMenu
		display none
		width 70px
		height 70px
		border-radius 50%
		background amarillo
		text-align center
		position absolute
		right 20px
		bottom -35px
		color azulSecundario
		p
			margin-top 25px
			font-weight bold
.rotar
	transform rotate(180deg)
	transition all 1s
            
